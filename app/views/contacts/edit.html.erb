<h1>Contacts#edit</h1>
<p>Find me in app/views/contacts/edit.html.erb</p>

<% form_for @contact, :url => { :controller => "contacts", :action => "update" } do |f| %>
	  <%= render :partial => "contact_form", :locals => { :f => f, :type => "edit" } %>
	  <%= f.submit "Save", :class => "uiButton" %> 
	  <%= link_to "Cancel", {:controller =>'contacts', :action => 'index'} , :class => "uiButton" %>
<% end -%>